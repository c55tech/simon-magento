{
    "name": "simon/integration",
    "description": "SIMON Integration module for Magento 2",
    "type": "magento2-module",
    "version": "1.0.0",
    "keywords": [
        "magento",
        "magento2",
        "module",
        "monitoring",
        "simon",
        "integration"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "SIMON Team",
            "email": "support@simon.example.com",
            "homepage": "https://simon.example.com"
        }
    ],
    "support": {
        "email": "support@simon.example.com",
        "issues": "https://github.com/your-org/simon/issues",
        "source": "https://github.com/your-org/simon",
        "docs": "https://github.com/your-org/simon/blob/main/modules/simon-magento/README.md"
    },
    "require": {
        "php": ">=8.1",
        "magento/framework": "^103.0 || ^104.0",
        "magento/module-backend": "*",
        "magento/module-store": "*"
